{
    "name": "takuya/php-7z-cmd-wrapper",
    "description": "7z Archiver Command Wrapper",
    "license": "GPL-3.0-or-later",
    "type": "library",
    "version": "0.1.1",
    "autoload": {
        "psr-4": {
            "SystemUtil\\Archiver\\": "src/"
        },
        "exclude-from-classmap": [
            "tests/*","Helpers/*"
        ]
    },
    "require": {
        "php": ">=7.3",
        "takuya/php-proc_open-wrapper": "^0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "takuya/php-mktempfile": "^1.0",
        "larapack/dd": "^1.1"
    },
    "autoload-dev": {
        "files": [
            "Helpers/helpers.php"
        ],
        "psr-4":{
            "Tests\\": "tests/"
        }
    }
}
